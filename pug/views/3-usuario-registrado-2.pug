        div.mt-4.col-md-8.pt-60-px-md.pt-10-px.align-items-center.d-flex.pl-35-px
            img(src="image/elemento-user.svg").img-w-5-vh-xs
            label.form-label.ps-2.ms-1.font-20-px.text-primary.mb-0.font-24-md.fw-bold JUAN PÉREZ MORENO
        .mt-85-px-md.mt-60-px.justify-content-between.d-flex.d-md-block
            a(href="").btn.btn-outline-primary.btn-lg.font-14-xs Si
            button(type="submit", disabled="true").btn.btn-primary.btn-lg.ms-md-4.continuar-btn.font-14-xs No
